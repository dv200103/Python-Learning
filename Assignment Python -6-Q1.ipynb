   "source": [
    "# WAP to Find the element in a given list is “Even” or “ODD” using List Comprehension.\n",
    "\n",
    "# List1= [12,23,34,45,56,67,78,89,90]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "4d8b5457",
   "metadata": {},
   "outputs": [],
   "source": [
    "List1= [12,23,34,45,56,67,78,89,90]\n",
    "comp = [f\"{i} even\" if i%2==0 else f\"{i} odd\" for i in List1]\n",
    "print(comp)"
   ]
  }
